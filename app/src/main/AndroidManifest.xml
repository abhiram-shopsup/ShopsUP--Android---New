<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hyperkonnect.shopsup">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.Activity_Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".modules.account.Activity_EditProfile" />
        <activity android:name=".modules.account.Activity_ContactUs" />
        <activity android:name=".modules.introScreens.Activity_Intro" />
        <activity android:name=".activities.Activity_NoNetwork" />
        <activity android:name=".modules.account.Activity_Terms" />
        <activity android:name=".modules.account.Activity_ActivityLog"></activity>
    </application>

</manifest>